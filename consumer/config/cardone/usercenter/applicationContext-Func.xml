<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="top.cardone.usercenter.func.FindOneUserByPrincipalFunc"
          class="top.cardone.usercenter.func.FindOneUserByPrincipalFunc"/>

    <alias name="top.cardone.usercenter.func.FindOneUserByPrincipalFunc" alias="findOneUserByPrincipalFunc"/>

    <bean id="top.cardone.usercenter.func.ReadOneUserIdFunc"
          class="top.cardone.usercenter.func.ReadOneUserIdFunc"/>

    <alias name="top.cardone.usercenter.func.ReadOneUserIdFunc" alias="readOneUserIdFunc"/>

    <bean name="readOneDepartmentFunc"
          class="top.cardone.data.func.ReadOneFunc">
        <property name="serviceBeanName" value="top.cardone.usercenter.service.DepartmentService"/>
    </bean>

    <bean name="readOneUserFunc"
          class="top.cardone.data.func.ReadOneFunc">
        <property name="serviceBeanName" value="top.cardone.usercenter.service.UserService"/>
    </bean>
</beans>