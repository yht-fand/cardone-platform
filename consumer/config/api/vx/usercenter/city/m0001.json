{
  "validation": {
    "rules": {
      "cityCode": {
        "required": true,
        "minlength": 4,
        "maxlength": 255
      },
      "name": {
        "required": true,
        "minlength": 4,
        "maxlength": 255
      }
    },
    "messages": {
      "cityCode": {
        "required": "城市编号必填",
        "minlength": "城市编号需{0}个字符以上",
        "maxlength": "城市编号需小于{0}个字符"
      },
      "name": {
        "required": "城市名称必填",
        "minlength": "城市名称需{0}个字符以上",
        "maxlength": "城市名称需小于{0}个字符"
      }
    }
  }
}