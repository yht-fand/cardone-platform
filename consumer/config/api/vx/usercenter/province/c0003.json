{
  "validation": {
    "rules": {
      "provinceCode": {
        "required": true,
        "minlength": 4,
        "maxlength": 255
      },
      "name": {
        "required": true,
        "minlength": 4,
        "maxlength": 255
      }
    },
    "messages": {
      "provinceCode": {
        "required": "省份编号必填",
        "minlength": "省份编号需{0}个字符以上",
        "maxlength": "省份编号需小于{0}个字符"
      },
      "name": {
        "required": "省份名称必填",
        "minlength": "省份名称需{0}个字符以上",
        "maxlength": "省份名称需小于{0}个字符"
      }
    }
  }
}