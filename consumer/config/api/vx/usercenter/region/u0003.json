{
  "validation": {
    "rules": {
      "regionId": {
        "required": true
      },
      "name": {
        "required": true,
        "minlength": 4,
        "maxlength": 255
      }
    },
    "messages": {
      "regionId": {
        "required": "区域标识必填"
      },
      "name": {
        "required": "区域名称必填",
        "minlength": "区域名称需{0}个字符以上",
        "maxlength": "区域名称需小于{0}个字符"
      }
    }
  }
}