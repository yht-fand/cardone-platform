<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">
    <!-- 用户 -->
    <bean name="top.cardone.usercenter.service.UserService" parent="providerParent">
        <property name="serviceInterface" value="top.cardone.usercenter.service.UserService"/>
        <property name="queue">
            <bean class="org.apache.activemq.command.ActiveMQQueue">
                <constructor-arg value="top.cardone.usercenter.service.UserService"/>
            </bean>
        </property>
    </bean>
</beans>