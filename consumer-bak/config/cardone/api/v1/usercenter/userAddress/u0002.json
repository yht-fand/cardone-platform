{
  "validation": {
    "rules": {
      "userAddressId": {
        "required": true
      },
      "name": {
        "required": true,
        "minlength": 4,
        "maxlength": 255
      }
    },
    "messages": {
      "userAddressId": {
        "required": "用户与地址标识必填"
      },
      "name": {
        "required": "用户与地址名称必填",
        "minlength": "用户与地址名称需{0}个字符以上",
        "maxlength": "用户与地址名称需小于{0}个字符"
      }
    }
  }
}